<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid
                android:color="@color/sectionborderColor">
            </solid>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid
                    android:color="@color/sectionLabelsColor">
                </solid>
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/toggle">
        <clip>
            <shape>
                <solid android:color="@color/sectionborderColor">

                </solid>
            </shape>
        </clip>
    </item>

</layer-list>